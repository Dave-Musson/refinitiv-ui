{
  "name": "@refinitiv-ui/solar-theme",
  "version": "5.0.0-alpha.1",
  "main": "index.less",
  "description": "Solar theme for the Element Framework",
  "scripts": {
    "build:charcoal": "theme-compiler charcoal --variant=charcoal",
    "build:pearl": "theme-compiler pearl --variant=pearl",
    "build": "npm run build:charcoal && npm run build:pearl",
    "watch": "watch \"npm run build\" src --wait=10",
    "prepack": "npm run build",
    "prepublishOnly": "tagver patch --filter '4.x' --branch 'v4' --base '4.0.0-0' && npm version from-git"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sami.int.thomsonreuters.com:elf/refinitiv-ui.git",
    "directory": "packages/solar-theme"
  },
  "author": "Refinitiv",
  "license": "BSD-3-Clause",
  "files": [
    "*",
    "!.*"
  ],
  "dependencies": {
    "@refinitiv-ui/elemental-theme": "^5.0.0-alpha.1"
  },
  "devDependencies": {
  },
  "publishConfig": {
    "registry": "https://bams-aws.refinitiv.com/artifactory/api/npm/default.npm.cloud"
  }
}
