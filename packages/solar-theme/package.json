{
  "name": "@refinitiv-ui/solar-theme",
  "version": "5.6.0",
  "description": "Solar theme for the Element Framework",
  "author": "Refinitiv",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.less",
  "exports": {
    "./charcoal/all-elements": "./charcoal/imports/all-elements.js",
    "./charcoal/custom-elements": "./charcoal/imports/custom-elements.js",
    "./charcoal/native-elements": "./charcoal/imports/native-elements.js",
    "./pearl/all-elements": "./pearl/imports/all-elements.js",
    "./pearl/custom-elements": "./pearl/imports/custom-elements.js",
    "./pearl/native-elements": "./pearl/imports/native-elements.js",
    "./charcoal/imports/all-elements": "./charcoal/imports/all-elements.js",
    "./charcoal/imports/custom-elements": "./charcoal/imports/custom-elements.js",
    "./charcoal/imports/native-elements": "./charcoal/imports/native-elements.js",
    "./pearl/imports/all-elements": "./pearl/imports/all-elements.js",
    "./pearl/imports/custom-elements": "./pearl/imports/custom-elements.js",
    "./pearl/imports/native-elements": "./pearl/imports/native-elements.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Refinitiv/refinitiv-ui.git",
    "directory": "packages/solar-theme"
  },
  "files": [
    "*",
    "!.*"
  ],
  "scripts": {
    "build:charcoal": "theme-compiler charcoal --variant=charcoal",
    "build:pearl": "theme-compiler pearl --variant=pearl",
    "build": "npm run build:charcoal && npm run build:pearl",
    "build:prod": "npm run build",
    "watch": "watch \"npm run build\" src --wait=10"
  },
  "dependencies": {
    "@refinitiv-ui/elemental-theme": "^5.4.0"
  },
  "devDependencies": {
    "@refinitiv-ui/theme-compiler": "^5.0.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
