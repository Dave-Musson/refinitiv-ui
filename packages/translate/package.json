{
  "name": "@refinitiv-ui/translate",
  "version": "5.4.0-alpha.2",
  "description": "i18n implementation for Element Framework components",
  "author": "Refinitiv",
  "license": "Apache-2.0",
  "main": "./lib/translate.js",
  "module": "./lib/translate.js",
  "types": "./lib/translate.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Refinitiv/refinitiv-ui.git",
    "directory": "packages/translate"
  },
  "scripts": {
    "build": "tsc --sourceMap --declarationMap",
    "build:prod": "tsc",
    "build:watch": "npm run build -- --watch --preserveWatchOutput",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "npm run build && concurrently \"npm run build:watch\" \"web-dev-server --config ../../server.config.js --app-index __demo__\"",
    "test": "npm run build && karma start ../../karma.config.js --package=translate",
    "test:snapshots": "npm run test -- --snapshots",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "lit": "~2.1.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@formatjs/intl-getcanonicallocales": "^1.8.0",
    "@formatjs/intl-locale": "^2.4.41",
    "@formatjs/intl-numberformat": "^7.2.6",
    "@formatjs/intl-pluralrules": "^4.1.6",
    "@refinitiv-ui/configurations": "workspace:^",
    "@refinitiv-ui/core": "workspace:^",
    "@refinitiv-ui/i18n": "workspace:^",
    "@refinitiv-ui/phrasebook": "workspace:^",
    "@refinitiv-ui/test-helpers": "workspace:^",
    "@types/node": "^16.11.7",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-legacy": "^0.1.7",
    "eslint": "^8.7.0",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "@refinitiv-ui/i18n": "workspace:^",
    "@refinitiv-ui/phrasebook": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  }
}
