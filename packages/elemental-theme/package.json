{
  "name": "@refinitiv-ui/elemental-theme",
  "version": "5.0.0-alpha.1",
  "description": "Base theme for Element Framework Components",
  "main": "index.less",
  "scripts": {
    "build:light": "theme-compiler light --variant=light",
    "build:dark": "theme-compiler dark --variant=dark",
    "build": "npm run build:light && npm run build:dark",
    "watch": "watch \"npm run build\" src --wait=10",
    "prepack": "npm run build",
    "prepublishOnly": "tagver  patch --filter '4.x' --branch 'v4' --base '4.0.0-0' && npm version from-git"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sami.int.thomsonreuters.com:elf/refinitiv-ui.git",
    "directory": "packages/elemental-theme"
  },
  "author": "Refinitiv",
  "license": "BSD-3-Clause",
  "files": [
    "*",
    "!.*"
  ],
  "dependencies": {},
  "devDependencies": {
  },
  "publishConfig": {
    "registry": "https://bams-aws.refinitiv.com/artifactory/api/npm/default.npm.cloud"
  }
}
